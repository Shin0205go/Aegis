# ============================================================================
# AEGIS 環境変数設定
# ============================================================================

# AI判定エンジン設定
LLM_PROVIDER=anthropic
LLM_MODEL=claude-sonnet-4-20250514

# APIキー（テスト用のダミー値）
ANTHROPIC_API_KEY=test-api-key

# LLM詳細設定
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=4096

# AI判定閾値設定
AEGIS_AI_THRESHOLD=0.7

# サーバー設定
PORT=3000
HOST=0.0.0.0

# ログ設定
LOG_LEVEL=info
LOG_FORMAT=json

# キャッシュ設定
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=100

# 開発環境設定
NODE_ENV=test